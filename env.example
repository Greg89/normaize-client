# App Configuration
VITE_APP_NAME=Normaize
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=Data Toolbox
VITE_NODE_ENV=development

# Auth0 Configuration
VITE_AUTH0_DOMAIN=your-domain.auth0.com
VITE_AUTH0_CLIENT_ID=your-client-id
VITE_AUTH0_AUDIENCE=your-api-identifier

# API Configuration
VITE_API_URL=http://localhost:5000
VITE_API_TIMEOUT=30000
VITE_API_RETRY_ATTEMPTS=3

# Logging Configuration (Seq)
VITE_SEQ_URL=https://your-seq-instance.railway.app
VITE_SEQ_API_KEY=your-seq-api-key
VITE_LOG_LEVEL=info

# Error Tracking (Sentry) - Only set in beta/production
# VITE_SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_PERFORMANCE_MONITORING=true
VITE_ENABLE_ERROR_TRACKING=false

# Build Configuration
VITE_SOURCE_MAP=true
VITE_MINIFY=true
VITE_TARGET_BROWSERS=defaults

# Development Configuration
VITE_DEV_SERVER_PORT=5173
VITE_DEV_SERVER_HOST=localhost 